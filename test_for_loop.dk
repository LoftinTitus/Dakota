\ Test for loop functionality

\ Basic for loop with range function
print("Testing for loop with range(5):")
for i in range(5):
    print(i)

print("Testing for loop with range(2, 7):")
for i in range(2, 7):
    print(i)

print("Testing for loop with range(0, 10, 2):")
for i in range(0, 10, 2):
    print(i)

\ For loop with matrix (iterate over rows)
print("Testing for loop with matrix:")
matrix = [1, 2; 3, 4; 5, 6]
for row in matrix:
    print(row)

\ For loop with simple integer (treats as range)
print("Testing for loop with integer:")
for i in 3:
    print("Count:", i)
